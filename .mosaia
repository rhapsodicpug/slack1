{
    "name": "Slack Chat History Fetcher",
    "description": "Fetches and returns recent chat history from a Slack channel",
    "schema": {
        "type": "function",
        "function": {
            "name": "Slack Chat History Fetcher",
            "description": "Fetches and returns recent chat history from a Slack channel",
            "strict": true,
            "parameters": {
                "type": "object",
                "properties": {
                    "channel_id": {
                        "type": "string",
                        "description": "The Slack channel ID to fetch messages from"
                    },
                    "user_id": {
                        "type": "string",
                        "description": "The Slack user ID requesting the chat history"
                    },
                    "text": {
                        "type": "string",
                        "description": "Optional number of messages to fetch (default: 50, max: 200)"
                    }
                },
                "required": [
                    "channel_id",
                    "user_id"
                ],
                "additionalProperties": false
            }
        }
    },
    "envVars": [
        "ENV_VAR_ONE"
    ],
    "server": {
        "startCommand": "npm run start:dev",
        "port": 3000,
        "endpoint": "/",
        "method": "GET"
    }
}