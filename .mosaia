{
    "name": "Slack Summary Bot",
    "description": "gives summary of your recents chats in one click",
    "schema": {
        "type": "function",
        "function": {
            "name": "Slack Summary Bot",
            "description": "gives summary of your recents chats in one click",
            "strict": true,
            "parameters": {
                "type": "object",
                "properties": {
                    "channel_id": {
                        "type": "string",
                        "description": "The Slack channel ID to summarize"
                    },
                    "user_id": {
                        "type": "string",
                        "description": "The Slack user ID requesting the summary"
                    },
                    "text": {
                        "type": "string",
                        "description": "Optional text to include with the summary request"
                    }
                },
                "required": [
                    "channel_id",
                    "user_id"
                ],
                "additionalProperties": false
            }
        }
    },
    "envVars": [
        "ENV_VAR_ONE",
        "ENV_VAR_TWO"
    ],
    "server": {
        "startCommand": "npm run start:dev",
        "port": 3000,
        "endpoint": "/",
        "method": "GET"
    }
}