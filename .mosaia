{
    "name": "Slack Chat History Poster",
    "description": "Fetches recent chat history from a Slack channel and posts it back to the channel",
    "schema": {
        "type": "function",
        "function": {
            "name": "Slack Chat History Poster",
            "description": "Fetches recent chat history from a Slack channel and posts it back to the channel",
            "strict": true,
            "parameters": {
                "type": "object",
                "properties": {
                    "channel_id": {
                        "type": "string",
                        "description": "The Slack channel ID to fetch messages from and post history back to"
                    },
                    "user_id": {
                        "type": "string",
                        "description": "The Slack user ID requesting the chat history"
                    },
                    "text": {
                        "type": "string",
                        "description": "Number of messages to fetch (default: 50, max: 200). Can be empty string to use default."
                    }
                },
                "required": [
                    "channel_id",
                    "user_id",
                    "text"
                ],
                "additionalProperties": false
            }
        }
    },
    "envVars": [
        "ENV_VAR_ONE"
    ],
    "server": {
        "startCommand": "npm run start:dev",
        "port": 3000,
        "endpoint": "/",
        "method": "GET"
    }
}